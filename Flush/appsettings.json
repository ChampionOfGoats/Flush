{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityDb": {
    "ConnectionString": "Data Source=data-sources/identity.enc",
    "HashAlgorithm": "SHA512",
    "Thumbprint": "identity_thumbprint"
  },
  "FlushDb": {
    "ConnectionString": "Data Source=data-sources/flush.enc",
    "HashAlgorithm": "SHA512",
    "Thumbprint": "flush_thumbprint"
  },
  "JwtAuthentication": {
    "HashAlgorithm": "SHA512",
    "Thumbprint": "jwt_thumbprint"
  }
}
